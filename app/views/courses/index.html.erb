<p id="notice"><%= notice %></p>

<h1>Listing Courses</h1>

<table>
  <thead>
    <tr>
      <th>Course name</th>
      <th>Course description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course.each do |course| %>
      <tr>
        <td><%= course.course_name %></td>
        <td><%= course.course_description %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_student_course_path %>
